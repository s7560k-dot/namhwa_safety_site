{
  "hosting": {
    "public": "frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/INDEX/resource.html",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/resource.html",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/safetydashboard.html",
        "destination": "/",
        "type": 301
      }
    ]
  }
}
